{
   "manifest_version": 3,
   "name": "Coeram DLsite Show All Works",
   "version": "1.0",
   "description": "DLsiteで「未購入作品のみ表示」のオプションを自動でオフにします",
   "icons": {
     "16": "icons/icon16.png",
     "48": "icons/icon48.png",
     "128": "icons/icon128.png"
   },
   "host_permissions": [
     "https://www.dlsite.com/*"
   ],
   "content_scripts": [
     {
       "matches": ["*://www.dlsite.com/*"],
       "js": ["src/content.js"],
       "run_at": "document_idle"
     }
   ]
 }
 